     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["The primary 120 MeV alpha beam was directed onto a 7"
		,"micron thick CD2 target, where it initiated"
		,"alpha-deuteron elastic scattering. The scattered"
		,"alpha beam was used in the experiment."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["5mm thick thick Si detector were used as target"]
	}
