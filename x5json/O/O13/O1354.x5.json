     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Targets of uranium and neptunium-oxide (236,238U(3)O(8)"
		,"and 237Np-O(2)) were prepared by multiple thermal"
		,"decomposition of uranium or neptunium nitrate cellulose"
		,"painted on 10-mum, 50-mum and 100-mum thick Al-backing."
		,"The average thickness of the actinide layer was 60-120"
		,"mug/cm**2 for the thin targets and 1.5-2-mg/cm**2 for"
		,"the thick targets. The thickness variations were within"
		," 3% for all types of targets used."]
	}
      ]
     ,"ADD-RES":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"TTY-C", "dict":"ADD-RES", "idict":20,"hlp":"Calculated thick target yield"}]
	,"x4freetext":[".The calculated thick-target yield of Np-235"
		," at 42.4-MeV projectile energy is 5.8-kbq/mua*hr."]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"CYCLO", "dict":"FACILITY", "idict":18,"hlp":"Cyclotron"}
	    ,{"code":"2SF JYV", "dict":"INSTITUTE", "idict":3,"hlp":"University of Jyvaskyla, Jyvaskyla, Finland"}]
	,"x4freetext":[" The K-130 cyclotron ."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"ISOCY", "dict":"FACILITY", "idict":18,"hlp":"Isochronous cyclotron"}
	    ,{"code":"2SF ABA", "dict":"INSTITUTE", "idict":3,"hlp":"Abo Akademi, Turku, Finland"}]
	,"x4freetext":[" The 103-cm AVF cyclotron - MGC-20"]
	}
