     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Targets used in this experiment consisted of"
		,"cylindrical pilot b plastic scintillators 1/8 in. thick"
		,"and 5/8 in. diam., containing 91.55% carbon by"
		,"weight."
		,"For each irradiation a target was carefully aligned"
		,"with and taped directly to the upstream side of"
		,"scintillation counters, which was the same size as"
		,"the target."
		,"Each one of the targets was irradiated in the proton"
		,"beam for approximately 20 min."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"TELES", "dict":"DETECTOR", "idict":22,"hlp":"Counter telescope"}
	    ,{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}
	    ,{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[" A scintillation counter telescope."
		,"1/8-in. thick and either 5/8 or 1 in. diam. was"
		,"optically coupled via a 6-in. -long by 1/8-in. -thick"
		,"Lucite light pipe to a RCA-8575 photomultiplier."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"NAICR", "dict":"DETECTOR", "idict":22,"hlp":"Sodium-Iodide crystal"}]
	,"x4freetext":[" two NaI(Tl) well crystals"]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" Starting about 10 min after the end of an"
		,"irradiation, the c-11 activity was measured either by"
		,"internal scintillation counting or by positron-"
		,"annihilation radiation counting."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"BCINT", "dict":"METHOD", "idict":21,"hlp":"Beam current integrated"}]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"EXTB", "dict":"METHOD", "idict":21,"hlp":"Irradiation with external beam"}]
	,"x4freetext":[" Pi+ Mesons contamination of the beam was less"
		,"than 4%."]
	}
      ]
