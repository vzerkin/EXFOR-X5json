     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"MAGSP", "dict":"DETECTOR", "idict":22,"hlp":"Magnetic spectrometer"}]
	,"x4freetext":[" The evaporated residues (ERs) yielded in the"
		,"fusion evaporation reaction 40Ar+186W were filtrated"
		,"by the Spectrometer for Heavy Atoms and Nuclear"
		,"Structure (SHANS)."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"PS", "dict":"DETECTOR", "idict":22,"hlp":"Position sensitive detector"}
	    ,{"code":"SISD", "dict":"DETECTOR", "idict":22,"hlp":"Silicon strip detector"}
	    ,{"code":"SI", "dict":"DETECTOR", "idict":22,"hlp":"Silicon detector"}
	    ,{"code":"MWPC", "dict":"DETECTOR", "idict":22,"hlp":"Multi-wire proportional counter"}]
	,"x4freetext":[" The ERs were implanted into three"
		,"300 um thick position-sensitive strip detectors (PSSDs)"
		,"with each active area of 50 X 50 mm2. Eight additional"
		,"non-position-sensitive side silicon detectors (SSDs)"
		,"were mounted upstream. The total detection efficiency"
		,"for the emitted alpha was measured to be about 72%."
		,"To distinguish the alpha events from the implanting"
		,"events, two multi-wire proportional counters (MWPCs)"
		,"were mounted 15 cm and 25 cm upstream from the PSSDs."
		,"Three extra non-position-sensitive silicon detectors,"
		,"featuring the same size as the PSSDs, were installed"
		,"side by side after the PSSDs to provide the veto"
		,"signals from the light particles passing through"
		,"the PSSDs."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"HPGE", "dict":"DETECTOR", "idict":22,"hlp":"Hyperpure Germanium detector"}]
	,"x4freetext":[" Near the charged particle detection system,"
		,"two High Purity Germanium (HPGe) detectors and one"
		,"clover detector were mounted at the right side,"
		,"downward, and downstream, respectively."]
	}
      ]
