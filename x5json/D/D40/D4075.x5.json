     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["the target used were natural argon gas of 1 bar"
		,"pressure in stainless steel cells with 10 micron"
		,"Ti windows."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"HPGE", "dict":"DETECTOR", "idict":22,"hlp":"Hyperpure Germanium detector"}]
