     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["natural metal Ti foils 1.8 mg/cm thick."
		,"Al foils were used as energy degrader in the stack."
		,"about 9 mg/cm2 vanadium pentoxid was prepared by"
		,"spattering onto thin Al foils."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}
	    ,{"code":"STTA", "dict":"METHOD", "idict":21,"hlp":"Stacked target irradiation"}
	    ,{"code":"GSPEC", "dict":"METHOD", "idict":21,"hlp":"Gamma ray spectrometry"}]
	,"x4freetext":[" stacks were irradiated in vacuum."
		,"beam current was monitored by Faraday-cup."
		,"the recoil los was about 20% and the cross section was"
		,"corrected for the recoil loss in the case of Ti"
		,"bombardment."]
	}
      ]
