     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Sm2O3 with natural isotopic abundances evaporated"
		,"by an electron beam onto 6um Al backing."
		,"The areal density of the Sm atoms was measured with"
		,"Rutherford backscattering spectrometry (RBS)."
		,"Thickness of the five targets was in the range of"
		," (0.5-1.0)E+18 atoms/cm2."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"LEGE", "dict":"DETECTOR", "idict":22,"hlp":"Low energy Germanium Detector"}]
	,"x4freetext":[" A thin crystal high-purity germanium detector"
		,"optimal for the detection of low-energy g-radiation."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" Irradiation was done in vacuum with 1 euA beam"
		,"of alpha particles. The lengths of irradiations"
		,"varied between 2 and 24hours."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"SITA", "dict":"METHOD", "idict":21,"hlp":"Single target irradiation"}]
	,"x4freetext":[" Targets were irradiated one by one."
		,"Decay data were taken from the latest nuclear data"
		,"compilation."
		,"The effective energy calculation is based on the"
		,"energy loss of the beam in the target layer."]
	}
