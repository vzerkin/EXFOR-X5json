     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" The fast neutron emission spectrum of Cf-252"]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".No information given."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" The IAEA Cf-252 neutron source consists of"
		," a cylindrical capsule with an outer length of 22.0"
		," mm and an external diameter of 2.80 mm, strength"
		," calibrated against a standard Cf-252 source."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REC", "dict":"METHOD", "idict":21,"hlp":"Collection of recoils"}]
	,"x4freetext":["   Measuring proton-recoils in the gas filled"
		," proportional counter spectrometers."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"PROPC", "dict":"DETECTOR", "idict":22,"hlp":"Proportional counter"}]
	,"x4freetext":[" A gas filled proportional counter filled"
		," with 200 kPa C-H4 gas, 200 kPa kripton gas and 0.1%"
		," He-3, energy calibrated with a Na-Be neutron source"
		," and the He-3(N,P)-reaction using thermal neutrons."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".The response functions for the detector were calcu-"
		," lated using the Monte-Carlo code MATRI, which is a"
		," modified version of the code SPEC-4."]
	}
      ]
     ,"STATUS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".Results taken from INDC(AUS)-4,,197912"]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".Correction for the high energy part of the Cf-252"
		," neutrons above 10 MeV applied."
		,".No gamma discrimination applied, since the lower"
		," detection threshold is 0.9 MeV."]
	}
      ]
