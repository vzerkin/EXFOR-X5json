     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["- APPARATUS AND METHODS USED IN A SLOW NEUTRON"
		,"  RESONANCE SCATTERING EXPERIMENT -"]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".BR1-REACTOR, CRYSTAL SPECTROMETER."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	,"x4freetext":["    BR1-REACTOR, NEUTRON BEAM FROM THE CORE."]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".MONOCHROMATIC BEAM OBTAINED FROM BRAGG-REFLECTION ON"
		," CU(111) PLANES."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".EVAPORATED INDIUM METAL ON ALUMINIUM"
		," BACKING OF 10 MICROMETERS. THREE DIFFERENT"
		," SAMPLES USED WITH DIAMETER OF 30 MILLIM."
		," AND THICKNESSES OF 2.08E18 ATOMS/CM*CM,"
		," 1.06E19 ATOMS/CM*CM, 1.31E19 ATOMS/CM*CM."
		,".SCATTERING SAMPLE AT RIGHT ANGLE TO NEUTRON BEAM,"
		," SURROUNDED BY PROPORTIONAL COUNTERS IN CYLINDRICAL"
		," GEOMETRY."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".MEASURING THE RATIO OF SCATTERING CROSS-"
		," SECTION TO THE TOTAL CROSS-SECTION AT"
		," RESONANCE ENERGY."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"PROPC", "dict":"DETECTOR", "idict":22,"hlp":"Proportional counter"}]
	,"x4freetext":["    EIGHT PROPORTIONAL COUNTERS FILLED WITH"
		," BORON-TRIFLUORIDE ENRICHED IN BORON-10,"
		," DETECTORS ARE 40 CM LONG, 5 CM IN DIAMETER,"
		," WITH 700 TORR PRESSURE."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".THE QUANTITY MEASURED IN THIS EXPERIMENT IS THE RATIO"
		," BETWEEN NEUTRON-WIDTH TO TOTAL WIDTH. THE STATISTICAL"
		," WEIGHT FACTOR AND THEREFORE THE SPIN OF THE COMPOUND"
		," NUCLEUS ARE OBTAINED USING RESONANCE PARAMETERS"
		," FROM BNL-325."]
	}
      ]
     ,"MONITOR":[
	{"x4pointer":" "
	 ,"x4code":{"code":"82-PB-0(N,EL),,SIG"
		,"reaction":"82-PB-0(N,EL),,SIG"}
	,"x4freetext":["  SCATTERING CROSS-SECTION OF LEAD,"
		," VALUE NOT GIVEN."]
	}
      ]
     ,"PART-DET":[
	{"x4pointer":" "
	 ,"x4codes":["N"]
	,"x4freetext":["    NEUTRONS"]
	}
      ]
     ,"COMMENT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".RESONANT SCATTERING INTENSITY OBTAINED FROM DIFFERENCE"
		," BETWEEN SAMPLE-IN AND SAMPLE-OUT COUNTING RATES AND"
		," NORMALIZED TO THE COUNTING RATE FROM A PB-SAMPLE."]
	}
      ]
