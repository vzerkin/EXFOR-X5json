     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"D-T", "dict":"INC-SOURCE", "idict":19,"hlp":"3H(d,n)"}]
	,"x4freetext":[" THE TRITIUM TARGET WAS ABSORBED IN A 0.4 MG/CM2"
		," THICK TITANIUM LAYER, EVAPORATED ON A 0.25 MM THICK"
		," COPPER BACKING. THIS TARGET WAS USED TO PRODUCE"
		," NEUTRONS FROM 14.6 TO 19.9 MEV."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"D-D", "dict":"INC-SOURCE", "idict":19,"hlp":"2H(d,n)"}]
	,"x4freetext":[" A SIMILAR DEUTERIUM TARGET WAS USED TO PRODUCE"
		," 5 MEV NEUTRONS."]
	}
      ]
