      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".LIGHT AND HEAVY WATER SAMPLES"
		,".IN CU - TUBES WITH 0.5 MM THICK CU - WINDOWS."
		,".SAMPLES CONTAINED THE SAME NUMBER OF O - ATOMS."
		,".HEAVY WATER CONSISTED OF 99.75 PERCENT D-2-O."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REC", "dict":"METHOD", "idict":21,"hlp":"Collection of recoils"}
	    ,{"code":"COINC", "dict":"METHOD", "idict":21,"hlp":"Coincidence"}]
	,"x4freetext":[".RECOIL PROTONS EMERGING FROM A POLYTHENE SCATTERER"
		," WERE DETECTED BY A TRIPLE COINCIDENCE COUNTER"
		," TELESCOPE."
		,".AN AL-ABSORBER WAS PLACED BETWEEN THE TWO LAST"
		," DETECTORS TO SET A LOW ENERGY LIMIT TO THE"
		," RECOIL PROTONS DETECTED."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[" COUNTER TELESCOPE CONSISTING OF"
		," 3 POLYSTYRENE CRYSTALS, EACH 7 MM THICK,"
		," MOUNTED ON TOP OF A PHOTOMULTIPLIER TUBE."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"BF3", "dict":"DETECTOR", "idict":22,"hlp":"Boron Trifluoride neutron detector"}]
	,"x4freetext":["  BF3 DETECTOR TO MONITOR THE NEUTRON BEAM."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".AN EFFECTIVE NEUTRON ENERGY WAS CALCULATED."
		,".THE ENERGY BAND WIDTH VARIED BETWEEN 19 AND"
		," 31 MEV DEPENDING ON THE ABSORBER THICKNESS IN"
		," THE DETECTOR TELESCOPE."]
	}
      ]
     ,"PART-DET":[
	{"x4pointer":" "
	 ,"x4codes":["P"]
	,"x4freetext":[".RECOIL PROTONS FROM A 17MM THICK POLYTHENE SCATTERER."]
