	 ,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	,"x4freetext":[" REACTOR NEUTRONS, FLUX 6 10**12 N/CM2/SEC, FOR"
