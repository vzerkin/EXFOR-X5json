     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"P-LI7", "dict":"INC-SOURCE", "idict":19,"hlp":"7Li(p,n)"}]
	,"x4freetext":[" 30 MG/CM-SQ THICK SELF-SUPPORTING FOIL OF"
		," NATURAL ABUNDANCE LOCATED AT THE EXIT OF THE BEAM"
		," SWINGER MAGNET. NEUTRONS OF 35 MEV (650 KEV FWHM) WERE"
		," PRODUCED AT 0 DEG TO THE PROTON BEAM WITH INCIDENT"
		," PROTON ENERGIES OF 37 MEV."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"TOF", "dict":"METHOD", "idict":21,"hlp":"Time-of-flight"}]
	,"x4freetext":["NEUTRON TIME-OF-FLIGHT SPECTROMETER WITH A BEAM"
		," SWINGER SYSTEM, USING A 44 M FLIGHT PATH."
		,".TO REDUCE BACKGROUND COMING FROM THE NEUTRON PRODUCING"
		," SOURCE IN THE FORWARD DIRECTION AT THE DETECTOR"
		," POSITION, 1 M THICK CONCRETE WALLS AND LEAD AND IRON"
		," COLLIMATORS, AS WELL AS A 90 CM LONG IRON SHADOW BAR,"
		," WERE USED."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":["AN ARRAY OF 12 LIQUID SCINTILLATOR DETECTORS:"]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"STANK", "dict":"DETECTOR", "idict":22,"hlp":"Scintillator tank"}]
	,"x4freetext":[" FOUR RECTANGULAR BOXES DIMENSIONS 50 X 10 X"
		," 5 CM FILLED WITH NE213, AND EIGHT CYLINDRICAL CON-"
		," TAINERS 20.3 CM DIAM AND 5 CM DEEP ALSO FILLED WITH"
		," NE213 LIQUID SCINTILLATOR."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".NATURAL C TARGET 2.0 CM DIAM AND 7.5 CM IN HEIGHT"
		," WEIGHING 39 G AND SUSPENDED ON NEGLIGIBLY THIN NYLON"
		," WIRES."]
	}
      ]
