     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REAC", "dict":"FACILITY", "idict":18,"hlp":"Reactor"}]
	,"x4freetext":[" HIGH FLUX REACTOR IN PETTEN."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	,"x4freetext":[" 24.5 KEV NEUTRONS WERE PRODUCED BY FILTERING"
		," THE REACTOR NEUTRONS IN ONE OF THE HORIZONTAL BEAM"
		," TUBES. THE 2.4 CM DIAMETER FILTER CONSISTED OF 22.5 CM"
		," IRON, 36 CM ALUMINIUM AND 6.5 CM SULPHUR. THE 24.5 KEV"
		," NEUTRON FLUX WAS 3.6 +/- 0.3 TIMES E+5 NEUTRONS/CM2"
		," AND SECOND."]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".QUASIMONOENERGETIC 24.5 KEV NEUTRON BEAM WITH A CON-"
		," TAMINATION OF A FEW PERCENT EPITHERMAL NEUTRONS."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"EXTB", "dict":"METHOD", "idict":21,"hlp":"Irradiation with external beam"}]
	,"x4freetext":[" THE 3-HE TARGET WAS IRRADIATED BY THE FILTERED"
		," 24.5 KEV NEUTRON BEAM."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" ACTIVATION OF GOLD FOILS WERE USED AS NEUTRON"
		," FLUX MONITOR."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"NAICR", "dict":"DETECTOR", "idict":22,"hlp":"Sodium-Iodide crystal"}]
	,"x4freetext":[" THE CAPTURE GAMMA-RAYS WERE MEASURED WITH A"
		," 3 X 3 INCH NAI/TL/ DETECTOR, PLACED 9.4 CM FROM THE"
		," TARGET AND PERPENDICULAR TO THE BEAM. THE DETECTOR WAS"
		," SHIELDED WITH 10 CM OF LEAD AND WITH HYDROGEN- AND"
		," BORON-CONTAINING MATERIALS."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[" ACTIVE SHIELDING AGAINST COSMIC RAYS WAS PRO-"
		," VIDED BY A LARGE DETECTOR, 1000 CM2, WHICH CONSISTED"
		," OF LAYERS OF SCINTILLATING PLASTIC SANDWICHED WITH 5"
		," CM THICK LAYERS OF LEAD. THE RESPONSE OF THE NAI"
		," DETECTOR FOR 20.6 MEV GAMMA-RAYS WAS MEASURED USING"
		," PROTON CAPTURE GAMMA-RAYS AT THE UTRECHT VDG ACCELER-"
		," ATOR. THE EFFICIENCY OF THE DETECTOR WAS CALCULATED"
		," WITH A MONTE CARLO SIMULATION COMPUTER CODE."]
	}
      ]
     ,"PART-DET":[
	{"x4pointer":" "
	 ,"x4codes":["G"]
	}
      ]
     ,"MONITOR":[
	{"x4pointer":" "
	 ,"x4code":{"code":"79-AU-197(N,G)79-AU-198,,SIG"
		,"reaction":"79-AU-197(N,G)79-AU-198,,SIG"}
	,"x4freetext":[" A STACK OF NINE 50"
		," MG/CM2 THICK AU FOILS, COVERED ON BOTH SIDES WITH 1 MM"
		," THICK CADMIUM SHEETS, WAS USED FOR NEUTRON FLUX DETER-"
		," MINATION. BY STUDYING THE ACTIVITIES THROUGH THE"
		," FOILS, CORRECTIONS FOR THE EPITHERMAL NEUTRONS COULD"
		," BE APPLIED."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".TO ABTAIN A SIZABLE 3-HE TARGET DENSITY, AN ALUMINIUM"
		," TARGET GAS CELL, 10.2 CM LONG, 3.0 CM IN DIAMETER AND"
		," WITH 0.3 CM THICK WALLS, WAS KEPT AT A TEMPERATURE OF"
		," 11.8 +/- 0.4 K AND A PRESSURE OF 23.0 +/- 0.4 BARS."
		," THIS RESULTED IN A 3-HE DENSITY OF 1.48 +/- 0.05 TIMES"
		," E+22 NUCLEI/CM3."]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".CORRECTION WAS APPLIED FOR THE EPITHERMAL NEUTRONS."]
	}
      ]
