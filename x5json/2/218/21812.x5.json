	}
      ]
     ,"REFERENCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"C,82ANTWER,,181,8209","stdFileName":"C,82ANTWER,,181,1982"
		,"year":1982,"typ":"C","ref":"C,82ANTWER"
		,"shortRef":"Conf: Conf.on Nucl.Data for Sci.and Technol.,Antwerp 1982, p.181 (1982)"}]
	}
      ]
     ,"AUTHOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"name":"M.DAROUZET","ini":"M.","nam":"Darouzet"}
	    ,{"name":"A.GIACOMETTI","ini":"A.","nam":"Giacometti"}
	    ,{"name":"R.GIRIEUD","ini":"R.","nam":"Girieud"}
	    ,{"name":"M.ROBIN","ini":"M.","nam":"Robin"}]
	}
      ]
     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["-EXPERIMENTAL DETERMINATION OF THE CROSS SECTIONS"
		," OF PU,AM AND CM ISOTOPES IN A LIGHT WATER"
		," REACTOR NEUTRON SPECTRUM. (IN FRENCH)."]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REAC", "dict":"FACILITY", "idict":18,"hlp":"Reactor"}]
	,"x4freetext":[" SWIMMING POOL REACTOR `MELUSINE`,"
		,"        CONTAINING AN ASSEMBLY OF 5 BY 5 FUEL PINS."
		,"        THE FUEL WAS URANIUM DIOXIDE ENRICHED TO"
		,"        2.8 PER CENT AND CLAD IN ALUMINIUM."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".THERMAL REACTOR SPECTRUM. THE WATER TEMPERATURE WAS"
		," 32 DEGREES CELSIUS."
		," A 4-GROUP REPRESENTATION OF THE SPECTRUM WAS MEASURED"
		,"     F1 = FLUX BETWEEN 0.9 MEV AND 10 MEV"
		,"     F2 = FLUX BETWEEN 5 KEV AND 0.9 MEV"
		,"     F3 = FLUX BETWEEN 0.625 EV AND 5 KEV"
		,"     F4 = FLUX BETWEEN 0.0 EV AND 0.625 EV"
		,"  F1/F4 = 1.7   F2/F4 = 2.6   F3/F4 = 1.8"]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".THE SAMPLE ISOTOPES WERE PRESENT AS TRACE ELEMENTS"
		," (0.1 PER CENT BY MASS) IN CERTAIN FUEL PELLETS IN"
		," THE CENTRAL FUEL PIN."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" SAMPLES IRRADIATED FOR 1021.5 HOURS AT FULL"
		,"        POWER. THE CROSS SECTIONS WERE DEDUCED FROM"
		,"        GAMMA,ALPHA AND MASS SPECTROMETRY OF THE"
		,"        IRRADIATED AND NON RADIATED PURE AND DOPED"
		,"        FUEL PELLETS."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".GAMMA,ALPHA AND MASS SPECTROMETERS. NO FURTHER"
		," DETAILS GIVEN IN THE REFERENCE."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".THE MEASURED CROSS SECTIONS CORRESPONDED TO THE"
		," NEUTRON SPECTRUM AT THE CENTRAL PIN OF A 5 BY 5"
		," LATTICE. THEY WERE CONVERTED INTO CROSS SECTIONS"
		," FOR THE CENTRAL PIN OF A PWR 17 BY 17 LATTICE."
		," THIS CALCULATION USED A 4-GROUP REPRESENTATION"
		," OF THE NEUTRON SPECTRUM IN THE CENTRE OF A"
		," 17 BY 17 LATTICE-"
		,"   F1/F4 = 1.7   F2/F4 = 2.4   F3/F4 = 1.5"
		," THE GROUP FLUXES F1,F2,F3,F4 ARE DEFINED UNDER"
		," KEYWORD INC-SPECT."]
	}
      ]
     ,"COMMENT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".THE REFERENCE CONTAINS A COMPARISON OF THE"
		," MEASURED RESULTS TO AVERAGE CROSS SECTIONS"
		," CALCULATED FROM DATA IN A LIBRARY IN USE AT"
		," CADARACHE."]
	}
      ]
     ,"STATUS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".DATA TAKEN FROM REFERENCE."]
	}
      ]
     ,"HISTORY":[
	{"x4pointer":" "
	 ,"x4code":{"code":"830127C", "date":"830127", "act":"C", "dict":"HISTORY", "idict":15
	     ,"actHelp":"Compiled at the data center"}
	,"x4freetext":[" A.P.T."]
	}
	,{"x4pointer":" "
	 ,"x4code":{"code":"830210E", "date":"830210", "act":"E", "dict":"HISTORY", "idict":15
	     ,"actHelp":"Transmitted to other data centers"}
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".MODULE `EVOGENE` OF THE `KAFKA` SYSTEM WAS USED TO"
		," CALCULATE COORECTION FACTORS FOR THE GAMMA AND ALPHA"
		," ACTIVITIES OF HEAVY ISOTOPES AND CERTAIN FISSION"
		," PODUCTS PRESENT IN THEIRRADIATED SAMPLES. `EVOGENE`"
		," USES 1-GROUP CROSS SECTIONS BASED ON THE RESULTS"
		," OF THE NEUTRONICS CODE `APOLLO`."]
	}
      ]
     ,"ERR-ANALYS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".NO DETAILS GIVEN."]
	}
      ]
