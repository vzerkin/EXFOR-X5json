     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Ten water-filled aluminium spheres, maximal"
		," eccentricity 0.15, diameters from 5 to 20 cm,"
		," covering a buckling range of 0.09 to 1.3 /cm2."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"PLSED", "dict":"METHOD", "idict":21,"hlp":"Pulse die-away"}]
	,"x4freetext":["    Pulse die-away in spherical tanks."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"BF3", "dict":"DETECTOR", "idict":22,"hlp":"Boron Trifluoride neutron detector"}]
	,"x4freetext":["    Two BF3-proportional counters, mounted"
		," outside the spheres."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" The decay constants from the different spherical tanks"
		," have been fitted to a second degree polynomial in the"
		," buckling. Each point was weighted by its variance."
		," The cross-section is derived from the zero degree term"]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" The decay curves have been corrected for"
		," dead time and coincidences in the time analyzer."
		," A small background."
		," Higher modes by an iterative computer program"
		," dropping one by one the first channels until a"
		," constant lambda (decay constant) results."
		," All data have been reduced on a constant temperature"
		," of 21.7 degrees-Celsius."
		," Other corrections turned out to be negligible."]
	}
      ]
     ,"ERR-ANALYS":[
	{"x4pointer":" "
	 ,"x4code":{"header":"DATA-ERR"
	     ,"headerHelp":"Error in value of quantity, defined under ERR-ANALYS"}
	,"x4freetext":[" The error is due to the standard errors in"
		," the fitting to the buckling polynomial."]
	}
