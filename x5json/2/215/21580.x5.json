     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["-DETERMINATION OF THE NEUTRON-NEUTRON SCATTERING"
		," LENGTH FROM THE H-2(N,NP)N REACTION AT BOMBARDING"
		," ENERGIES BETWEEN 17 MEV AND 27 MEV.-"]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ISOCY", "dict":"FACILITY", "idict":18,"hlp":"Isochronous cyclotron"}
	    ,{"code":"2GERBON", "dict":"INSTITUTE", "idict":3,"hlp":"Univ. of Bonn, Germany"}]
	,"x4freetext":[" ISOCHRONOUS CYCLOTRON."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"D-D", "dict":"INC-SOURCE", "idict":19,"hlp":"2H(d,n)"}]
	,"x4freetext":[" 28 MEV DEUTERON BEAM (10 MICRO-AMPERES)"
		," ONTO A LIQUID NITROGEN COOLED HIGH PRESSURE"
		," (90 BAR) DEUTERIUM GAS TARGET."
		," NEUTRONS EMITTED IN FORWARD DIRECTION WERE"
		," COLLIMATED TO PRODUCE A BEAM OF 22 MM DIAM WITH"
		," AN INTENSITY OF 1.65E+06 NEUTRONS/MEV*CM*CM*S"
		," ON THE TARGET FOR THE HIGHEST ENERGIES."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".C-D(2) TARGET, 50 MG/CM*CM THICK IN THE FIRST,"
		," AND 30 MG/CM*CM IN THE SECOND AND THIRD PART OF"
		," THE EXPERIMENT. IT HAD THE FORM OF AN ELLIPSE"
		," WITH HALF-AXES OF 15 MM AND 10 MM, RESPECTIVELY,"
		," SO THAT IT APPEARED AS A CIRCULAR DISC TO THE BEAM."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"TOF", "dict":"METHOD", "idict":21,"hlp":"Time-of-flight"}]
	,"x4freetext":[" TO DETERMINE THE INCOMING NEUTRON ENERGY,"
		," AND TO DETERMINE THE PROTON ENERGY (TO DISCRIMINATE"
		," AGAINST DEUTERONS AND OTHER CHARGED PARTICLES)."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"PHD", "dict":"METHOD", "idict":21,"hlp":"Pulse-height discrimination"}]
	,"x4freetext":[" PULSE-HEIGHT DISCRIMINATION TO DO THIS"
		," REJECTION OF OTHER CHARGED PARTICLES ON THE"
		," BASIS OF THEIR TOF."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"REC", "dict":"METHOD", "idict":21,"hlp":"Collection of recoils"}]
	,"x4freetext":[" PROTON RECOILS ARE MEASURED TO MONITOR"
		," THE INCOMING NEUTRON FLUX."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"PSD", "dict":"METHOD", "idict":21,"hlp":"Pulse-shape discrimination"}]
	,"x4freetext":[" PULSE SHAPE DISCRIMINATION IS USED TO"
		," DISTINGUISH NEUTRONS FROM GAMMA-RAYS."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"TELES", "dict":"DETECTOR", "idict":22,"hlp":"Counter telescope"}]
	,"x4freetext":[" PROTON RECOIL TELESCOPE TO MONITOR THE"
		," INCOMING NEUTRON FLUX."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"COIN", "dict":"DETECTOR", "idict":22,"hlp":"Coincidence counter arrangement"}
	    ,{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}
	    ,{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[" BREAK-UP NEUTRONS ARE DETECTED AT"
		," 50 DEGREES WITH A WELL SHIELDED NE213 SCINTILLATOR"
		," IN COINCIDENCE WITH BREAK UP PROTONS EMITTED AT 48"
		," DEGREES ON THE OPPOSITE SIDE OF THE BEAM. THESE"
		," PROTONS ARE DETECTED IN A NE104 PLASTIC"
		," SCINTILLATOR."
		," THE NE104 SCINTILLATOR HAD A DIAM OF 110 MM."
		," THE NE213 SCINTILLATOR WAS 76 MM THICK, WITH A"
		," DIAMETER OF 127 MM."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"COIN", "dict":"DETECTOR", "idict":22,"hlp":"Coincidence counter arrangement"}
	    ,{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}
	    ,{"code":"SOLST", "dict":"DETECTOR", "idict":22,"hlp":"Solid-state detector"}]
	,"x4freetext":[" IN A 2ND+3TH PART OF THE EXPERIMENT"
		," THE NE104 PLASTIC SCINTILLATOR WAS REPLACED WITH"
		," SIX 450 MM*MM SI SURFACE BARRIER DETECTORS, MOUNTED"
		," IN TWO VERTICAL ROWS AT 47. AND 49. DEGR"
		," RESPECTIVELY, IN EACH OF WHICH THE THREE DETECTORS"
		," WERE ELECTRONICALLY TREATED AS ONE."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[" THE PROTONS PASS THROUGH A SCINTILLATION"
		," FOIL OF 1.5 MG/CM*CM TO PRODUCE THE TOF START"
		," SIGNALS FOR THE PROTON ENERGY MEASUREMENT."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".IS DONE BY FITTING THE RESULTS OF MONTE CARLO"
		," CALCULATIONS OF THE PROCESS IN POINT GEOMETRY"
		," FOR DIFFERENT POSSIBLE VALUES OF THE SCATTERING"
		," LENGTH TO THE EXPERIMENTAL DATA."
		," FOR MORE DETAILS ABOUT THESE MONTE CARLO"
		," CALCULATIONS, SEE NP/A,329,1,141."]
	}
      ]
     ,"PART-DET":[
	{"x4pointer":" "
	 ,"x4codes":["P"]
	,"x4freetext":[" BREAK-UP PROTONS."]
	}
	,{"x4pointer":" "
	 ,"x4codes":["N"]
	,"x4freetext":[" BREAK UP NEUTRONS."]
	}
      ]
     ,"COMMENT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".THE EFFECTIVE RANGE WAS KEPT FIXED AT ITS CHARGE"
		," SYMMETRIC VALUE OF 2.8 FM. VARIATION OF THIS VALUE"
		," BETWEEN 2.5 AND 3.5 FM DOES NOT CHANGE THE"
		," SCATTERING LENGTH BY MORE THAN 0.1 FM."]
	}
      ]
