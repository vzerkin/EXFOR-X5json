     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" Polarized (14.6+-3.0 %) boron sample of 3.8 g."
		," 90% B-10, 10% B-11. Molten with 99.9% pure Al."
		," 2.51 cm diameter, 0.15 cm thickness."
		," Ti-49 target was mounted against the Al(B) target."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"GELI", "dict":"DETECTOR", "idict":22,"hlp":"Germanium-Lithium detector"}]
	,"x4freetext":[" . At 0 and 90 degrees relative to polarizing"
		,"magnetic field . Relative efficiency 15 % ,"
		,"FWHM at 1.33 MeV - 2.6 keV and 2.7 keV for vertical and"
		,"horizontal detector, respectively."
		,"478 keV gamma emitted by excited Li-7 was used to"
		,"determine detectors response."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"FISCH", "dict":"DETECTOR", "idict":22,"hlp":"Fission chamber"}]
	,"x4freetext":[" Fission counter for total neutron flux;"
		,"placed behind the sample."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"BF3", "dict":"DETECTOR", "idict":22,"hlp":"Boron Trifluoride neutron detector"}]
	,"x4freetext":[" 2nd Heusler crystal and BF3 counter, placed"
		,"behind the target, - to determine neutron polarization."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" Spectra in 2560 channels in energy region 250-640 keV"
		,"were measured at two angles."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" Relative asymmetry e(THETA) at angle THETA was defined"
		,"e(THETA)="
		," (I(THETA)par-I(THETA)anti)/(I(THETA)par+I(THETA)anti),"
		,"where I(THETA) is counting rate with target and neutron"
		,"spin parallel (par) and antiparallel (anti) ."
		,"e(0deg)=e(90deg)=0.070(2) was derived from the peak"
		,"area of the 478 keV transition."]
	}
      ]
