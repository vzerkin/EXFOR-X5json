     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["-NEUTRON TOTAL CROSS SECTION MEASUREMENT OF THORIUM"
		," AVERAGED NEAR 24 KEV WITH AN FE-FILTERED BEAM-"]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"LINAC", "dict":"FACILITY", "idict":18,"hlp":"Linear accelerator"}]
	,"x4freetext":["   46 MEV ELECTRON LINAC OF THE RESEARCH"
		," REACTOR INSTITUTE FROM KYOTO UNIVERSITY."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"PHOTO", "dict":"INC-SOURCE", "idict":19,"hlp":"Photo-neutron"}]
	,"x4freetext":["   WATER COOLED PHOTONEUTRON TARGET MADE OF"
		," TANTALUM PLATES WITH A 5 CM THICK POLYETHYLENE"
		," MODERATOR."
		,".A PURE IRON FILTER OF 30 CM THICKNESS WAS USED FOR"
		," FILTERING THE NEUTRON BEAM."]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".IRON FILTERED NEUTRON SPECTRUM PEAKED AT 24.3 KEV"
		," WITH AN ENERGY WIDTH OF ABOUT 2 KEV."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".SIX THORIUM METALIC PLATES OF DIFFERENT THICKNES"
		," WERE USED RANGING FROM 0.0273 TO 0.1551 ATOMS/BARN."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"TOF", "dict":"METHOD", "idict":21,"hlp":"Time-of-flight"}]
	,"x4freetext":["  22.2 M FLIGHT PATH."
		,".IRON FILTERED BEAM."
		,".TRANSMISSION MEASUREMENT."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"NAICR", "dict":"DETECTOR", "idict":22,"hlp":"Sodium-Iodide crystal"}]
	,"x4freetext":["    FOR NEUTRON DETECTION A BARE 12.7 CM"
		," DIAM. BY 5.08 CM THICK NAI(TL) CRYSTAL WAS USED."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"GLASD", "dict":"DETECTOR", "idict":22,"hlp":"Glass detector"}]
	,"x4freetext":["    A LI-6 GLASS SCINTILLATOR WAS USED FOR"
		," DETERMINING INTERFERENCE EFFECTS IN RESONANCE CROSS"
		," SECTIONS BETWEEN IODINE IN DETECTOR AND THORIUM"
		," SAMPLE."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"PROPC", "dict":"DETECTOR", "idict":22,"hlp":"Proportional counter"}]
	,"x4freetext":["   A BF3 PROPORTIONAL COUNTER WAS USED FOR"
		," MONITORING THE NEUTRON INTENSITY."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".CROSS SECTION OBTAINED BY EXTRAPOLATIONS OF"
		," TRANSMISSION FACTOR TO ZERO SAMPLE THICKNESS."]
	}
      ]
     ,"COMMENT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".CROSS SECTION MEASURED WITH GREAT ACCURACITY."
		,".CALCULATIONS OF POTENTIAL AND RESONANCE CROSS"
		," SECTIONS WERE MADE WITH DATA TAKEN FROM LITERATURE,"
		," SEE DETAILS IN REFERENCE."
		,".RECOMENDED AVERAGE PARAMETERS ARE GIVEN."]
	}
      ]
