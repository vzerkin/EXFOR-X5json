	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"PHOTO", "dict":"INC-SOURCE", "idict":19,"hlp":"Photo-neutron"}]
	,"x4freetext":["  WATER COOLED PHOTONEUTRON TARGET MADE OF"
		," TANTALUM PLATES WITH A 5 CM THICK POLYETHYLENE"
		," MODERATOR."
		,".A PURE IRON FILTER OF 30 CM THICKNESS WAS USED FOR"
		," FILTERING THE NEUTRON BEAM."]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".IRON FILTERED NEUTRON SPECTRUM PEAKED AT 24.3 KEV"
		," WITH AN ENERGY WIDTH OF ABOUT 2 KEV."
		,".LOWER ENERGY LIMIT = 17.5 KEV"
		,".UPPER ENERGY LIMIT = 26.3 KEV"]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".PURE WATER REPLACED BY A OPTICALY FLAT SIO2 PLATE."
		,".PLATE THICKNESS = 1 AND 2 CM."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"TOF", "dict":"METHOD", "idict":21,"hlp":"Time-of-flight"}]
	,"x4freetext":["  22.2 M FLIGHT PATH."
		,".IRON FILTERED BEAM."
		,".TRANSMISSION MEASUREMENT."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"NAICR", "dict":"DETECTOR", "idict":22,"hlp":"Sodium-Iodide crystal"}]
	,"x4freetext":["  FOR NEUTRON DETECTION A BARE 12.7 CM"
		," DIAM. BY 5.08 CM THICK NAI(TL) CRYSTAL WAS USED."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"PROPC", "dict":"DETECTOR", "idict":22,"hlp":"Proportional counter"}]
	,"x4freetext":["  A BF3 PROPORTIONAL COUNTER WAS USED FOR"
		," MONITORING THE NEUTRON INTENSITY."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".EFFECTIVE NEUTRON ENERGY CALCULATED BY FORMULA 1"
		," OF REFERENCE, SEE DETAILS IN P.5 OF TEXT."
		,".N-P CROSS SECTION CALCULATED BY SUBSTRACTING"
		," CONTRIBUTIONS OF OXIGEN AND SIO2 WHOSE CROSS"
		," SECTIONS WERE TAKEN FROM R.C.BLOCK ET.AL. J. OF"
		," NUCL.SCI.TECHN.12(1975)1."]
	}
      ]
     ,"COMMENT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".PRESENT RESULT IS VERY CLOSE TO THAT OBTAINED WITH"
		," THE EFFECTIVE RANGE FORMULA USING THE LOMON AND"
		," WILSON PARAMETER VALUES,PHYS.REV. C9 (1974) 1392,"
		," AND IS FAR FROM THE DILG VALUES,PHYS. REV. C11,"
		," (1975) 103."
		,".CROSS SECTION MEASURED WITH GREAT ACCURACITY."]
	}
      ]
