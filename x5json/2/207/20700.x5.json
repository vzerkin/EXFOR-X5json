     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["-MEASUREMENT OF THE NEUTRON-PROTON TOTAL CROSS"
		," SECTION USING 24-KEV IRON FILTERED NEUTRONS-"]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"LINAC", "dict":"FACILITY", "idict":18,"hlp":"Linear accelerator"}]
	,"x4freetext":["    46 MEV ELECTRON LINAC OF THE RESEARCH"
