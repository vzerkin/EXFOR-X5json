     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"PHOTO", "dict":"INC-SOURCE", "idict":19,"hlp":"Photo-neutron"}]
	,"x4freetext":[" SB-BE PHOTONEUTRON SOURCE CALIBRATED ABSOLUTELY"
		,"        IN A MANGANESE BATH TO +-0.5 PC. HALF-LIFE OF S"
		,"        124 TAKEN TO BE 60.2+-0.03 D TO COMPUTE CHANGES"
		,"        OF SOURCE STRENGTH WITH TIME. SOURCE STRENGTH A"
		,"        THE TIME OF IRRADIATION WAS 7.37X10**6 N/SEC."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".CSI CRYSTAL OF DIAMETER 2.54 CM AND THICKNESS 0.38 CM."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".CROSS SECTION EXTRACTED BY FITTING A STRAIGHT LINE TO"
		," THE VARIATION OF I-128 ACTIVITY WITH INVERSE SQUARE OF"
		," THE DISTANCE FROM THE SOURCE."]
	}
      ]
     ,"DECAY-DATA":[
	{"x4pointer":" "
	 ,"x4code":{"Nucl":"51-SB-124", "t12":60.2, "t12units":"D", "t12sec":5201280.0
	  ,"Radiations":[{"Type":"B-"}]}
	,"x4freetext":[" FROM LITERATURE."]
	}
      ]
