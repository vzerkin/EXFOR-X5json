     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"SPALL", "dict":"INC-SOURCE", "idict":19,"hlp":"Spallation"}]
	,"x4freetext":[" Spallation reaction in a mercury target"
		,"irradiated with 3-GeV protons (150 kW) . The proton"
		,"beams were supplied at a repetition rate of 25 Hz and"
		,"in the \"double-bunch mode\". Produced neutrons were"
		,"slowed down in a supercritical para hydrogen moderator."]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Relative neutron flux was reconstructed using the 478"
		,"keV gamma-rays from gamma rays from the B-10"]
	}
	,{"x4pointer":" "
