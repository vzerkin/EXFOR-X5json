     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".THERMAL NEUTRONS FROM REACTOR. IRRADIATIONS WERE PER-"
		," FORMED BOTH WITH AND WITHOUT CD SHIELDS."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" THE RESONANCE INTEGRAL WAS MEASURED USING THE CD-RATI"
		," METHOD."
		,".CO/AL AND AU/AL ALLOYS WERE USED TO DETERMINE THE"
		," NEUTRON FLUX AND THE WESTCOTT EPITHERMAL INDEX. THE"
		," NEUTRON FLUX WAS FOUND TO BE 4.62E+11 +/- 0.08E+11"
		," NEUTRONS/CM2/SEC, AND THE EPITHERMAL INDEX 0.033."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"HPGE", "dict":"DETECTOR", "idict":22,"hlp":"Hyperpure Germanium detector"}]
	,"x4freetext":[" THE CAPTURE GAMMA-RAY ACTIVITIES WERE MEASURED"
		," WITH A HPGE DETECTOR OF 90 PERCENT EFFICIENCY COMBINE"
		," WITH FAST ELECTRONICS."]
	}
      ]
     ,"PART-DET":[
	{"x4pointer":" "
	 ,"x4codes":["DG"]
	,"x4freetext":["    540 AND 591 KEV GAMMA-RAYS FROM TC-100."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".THE FOUR TARGETS EACH CONTAINED ABOUT 370 KBQ OF"
		," TC-99. THE SPECIFIC ACTIVITY OF THE TC-99 WAS TARGET"
		," SOLUTION WAS DETERMINED BY MEASURING THE SPECIFIC"
		," BETA-ACTIVITY BY THE LIQUID SCINTILLATION COUNTING"
		," METHOD, AND WAS FOUND TO BE 461 KBQ/G."]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".CORRECTIONS WERE MADE FOR EPITHERMAL NEUTRONS."]
	}
      ]
