     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".VERY PURE THERMAL SPECTRUM. RATIO OF THE NUMBER"
		," OF SLOW NEUTRONS TO THAT OF EPITHERMAL AND FAST"
		," NEUTRONS ABOUT 10E+6. DIRECT GAMMA-RAY FLUX FROM"
		," REACTOR REDUCED BY A FACTOR  1.E+6."
		,".VERY INTENSE NEUTRON FLUX =  1.E+9 CM-2.SEC-1."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".U238 LAYER PREPARED BY EVAPORATION OF UF4 ON A THIN"
