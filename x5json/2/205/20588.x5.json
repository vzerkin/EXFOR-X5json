		," +-10 MICRO-G/CM**2."
		,".ISOTOPIC COMPOSITION IN ATOMS PERCENT ="
		," 0.00016+-0.00001 OF U234, 0.01755+-0.00005 OF U235,"
		," LESS THAN 0.00001 OF U236,"
		," 99.9823+-0.0001 OF U238."]
	}
