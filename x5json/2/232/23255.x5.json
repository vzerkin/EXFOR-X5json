     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"D-D", "dict":"INC-SOURCE", "idict":19,"hlp":"2H(d,n)"}]
	,"x4freetext":["  Thin deuterated target composed of a Ti layer"
		,"saturated by deuterium 825microg/cm2 thick attached to"
		,"a 0.5mm thick silver backing."
		,"  Characteristics of the deuteron beams:"
		,"Neutron energy.MeV             3.7   4.3    5.4    7."
		,"Deuteron beam current,MicroA   5.    8.     13.    2.5"
		,"Deuteron beam energy,keV       765.  1234.  2174.  3278"
		,"Mean energy uncertainty,keV    0.8   1.     1.5    2."
		,"FWHM of peak, %                8.6   4.8    2.5    9.2"]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" Mono-energetic neutrons."
		,"PLC measurements were performed at 5 m and 0deg from"
		,"the target both with and without a shadow cone to"
		,"measure the fluence of the direct neutrons. The neutron"
		,"fluence was monitored both by integrating the deuteron"
		,"charge deposited on the target and by means of two De"
		,"Pangher long counters placed at a distance of 6 m from"
		,"the target."
		,"The fluence at the calibration position was determined"
		,"with an uncertainty of 2% to 3%."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Metal of natural composition."
		,"Two terbium samples :"
		," - 30mm diameter disk, 5mm of thickness, total mass"
		,"28.9g;"
		,"- parallelepiped shape, 5x8x10 mm,total mass of 3.3g"]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" Terbium samples were irradiated under 0deg"
		,"angle with respect to the incoming deuteron beam."
		,"The sample-to-target distance was varied from 3.3cm to"
		,"20.3cm leading to neutron fluence rates between"
		,"6.E+4 and 3.E+6 /cm2/s ."
		,"Volumetric inhomogeneity (i.e. the maximum variation)"
		,"in the terbium disk sample was of 39%, 44% and 53% in"
		,"fluence and of 54keV, 86keV and 133keV in energy,"
		,"respectively at 3.7MeV, 4.3MeV, and 5.4MeV."
		,"The irradiations in the 7MeV neutron field were"
		,"performed at three distances:"
		,"Distance to the target,cm    15.3  3.3   10.3"
		,"Volumetric inhomogeneity,%   14.   93.   22."
		,"Neutron energy error,kev     27.   413.  48."
		,"Counting times between 15 minutes and 17 hours at"
		,"1.5cm source-detector distance."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"LONGC", "dict":"DETECTOR", "idict":22,"hlp":"Long counter"}]
	,"x4freetext":[" Long counters for reference fluence"
		,"verification."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"HPGE", "dict":"DETECTOR", "idict":22,"hlp":"Hyperpure Germanium detector"}]
	,"x4freetext":[" HPGe detector GC1212, CANBERRA for gamma-ray"
		,"spectroscopy of samples immediately after irradiation."
		,"The extra-range germanium detector GX4019, CANBERRA at"
		,"KINR ( Ukraine) after 5 days ."
		,"The energy resolution was found to be 1.9keV for 1332"
		,"keV gamma-rays from 60Co ; 0.8keV for 122keV from 57Co."
		,"The efficiency calibration of the detector was"
		,"performed by 60Co, 137Cs, 152Eu, 241Am sources."]
	}
      ]
