     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["   Zr-97 yield was calculated as"
		,"  (NiLAMBLAi/(N238*LAMBLAf))*100, where"
		,"NiLAMBLAi - equilibrium activity of the fission product"
		,"with mass number i,"
		,"N238 - number 0f U-238 atoms,"
		,"LAMBDAf - U-238 spontaneous fission decay constant,"
		," (7.8+-0.9)*10**17 1/yr."
		,"Equilibrium activity of Zr-97 :"
		,"1st run - (3.67+-0.37)*10**-4 dis/sec per g U-238,"
		,"2nd run - (3.96+-0.4)*10**-4 dis/sec per g U-238."]
	}
