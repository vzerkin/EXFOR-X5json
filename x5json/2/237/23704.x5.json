     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".Pitchblende from Eldorado Mines, Great Bear Lake,"
		,"Canada, ~50% U(3)O(8) - for long-lived nuclides."
		,"300 g - for Sr."
		,".Pure UO(2)(NO(3))2*6H(2)O ."
		,"900 g for Cs, 2.3 kg for Ba."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["     Phillips low-background assembly of pancake type"
		,"G.M.Sonde 18516 Sample counter with a Fe-Cr window of"
		,"10 mg/cm2 thickness. A stainless steel dome shaped"
		,"anti-coincidence umbrella, filled with Ar and halogen"
		,"quencher - as ring counter."
		,"Counting efficiency was determined by Sr-90, Y-90,"
		,"P-32, Na-22 samples of various thickness with"
		,"uncertainty 10-15 %."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"CHSEP", "dict":"METHOD", "idict":21,"hlp":"Chemical separation of products"}]
	,"x4freetext":["                      Sr-89 Sr-90 Cs-136 Cs-136 Ba-139"
		,"U-238,kg              0.173 0.173 0.763  0.371  1.12"
		,"Chem.yield,%          50.4  50.4  30.3   28.2   45.1"
		,"Counting efficiency,% 35.0  13.5  37.5   38.0   35.2"
		,"                      +-    +-    +-     +-     +-"
		,"                      1.0   0.3   1.0    1.0    0.9"
		,"Activity at t=0,cpm   0.5   0.3   2.8    1.3    4.8"
		,"                      +-    +-    +-     +-     +-"
		,"                      0.2   0.1   0.5    0.3    0.8"
		,"Equilibrium activity, 2.7   4.3   5.5    5.6    4.5"
		,"dis/sec/gU x10**-4    +-    +-    +-     +-     +-"
		,"                      0.9   1.4   1.1    1.2    1.0"]
	}
