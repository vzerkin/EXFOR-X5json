     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".Spherical shell,  outer diam.14.99 cm,"
		," inner diam. 13.46 cm. For e(N) = 3.2, 3.7, 4.2"
		," and 5.2 MeV in addition a measurement was done"
		," with a shell with outer diam. 14.99 cm and"
		," inner diam. 11.94 cm."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".Shell transmission."]
