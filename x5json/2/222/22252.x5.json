     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[" Measurement of production cross sections of short"
		," lived nuclei by cyclic activation using a pulsed"
		," neutron source"]
	}
      ]
     ,"AUTHOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"name":"J.Yamamoto","ini":"J.","nam":"Yamamoto"}
	    ,{"name":"J.Datemichi","ini":"J.","nam":"Datemichi"}
	    ,{"name":"T.Kanaoka","ini":"T.","nam":"Kanaoka"}
	    ,{"name":"T.Takayama","ini":"T.","nam":"Takayama"}
	    ,{"name":"A.Takahashi","ini":"A.","nam":"Takahashi"}
	    ,{"name":"K.Sumita","ini":"K.","nam":"Sumita"}]
	}
      ]
     ,"INSTITUTE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"2JPNOSA", "dict":"INSTITUTE", "idict":3,"hlp":"Osaka Univ., Osaka, Japan"}]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ACCEL", "dict":"FACILITY", "idict":18,"hlp":"Accelerator"}
	    ,{"code":"2JPNOSA", "dict":"INSTITUTE", "idict":3,"hlp":"Osaka Univ., Osaka, Japan"}]
	,"x4freetext":[" The Osaka University Accelerator used."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"D-T", "dict":"INC-SOURCE", "idict":19,"hlp":"3H(d,n)"}]
	,"x4freetext":[" A pulsed 14 MeV neutron source was produced"
		," by bombarding a tritium target with a D+ beam."]
	}
      ]
     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".The neutron activation was made cyclic. The duration"
		," of the bursts was controlled by the D+ beam pulsing."
		," The beam controller was interlocked with the activity"
		," counting system and was designed as to continuously"
		," change the neutron burst time and the period between"
		," bursts from 0.3ms to 54.3 ms and 0.6 ms to 108.6 ms,"
		," respectively."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"ACTIV", "dict":"METHOD", "idict":21,"hlp":"Activation"}]
	,"x4freetext":[" The neutron (cyclic) activation technique was"
		," used to measure the cross-sections."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"TOF", "dict":"METHOD", "idict":21,"hlp":"Time-of-flight"}]
	,"x4freetext":[" The neutron energy spectrum of the irradiating"
		," source was analysed by TOF technique using an NE213"
		," scintillation detector as a monitor."]
	}
      ]
     ,"COMMENT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".At present, the technique is applicable only to"
		," isotopes that emit gamma-rays."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".Samples of natural lead were used, positioned at 15 cm"
		," from the neutron source and at 75 cm from the Ge"
		," detector. They were either disks of 45 mm diameter and"
		," 2 mm in thickness or hollow cylinders of dimensions,"
		," 28 mm inner diameter, 30 mm in outer diameter, 70 mm"
		," length and 1 mm thickness."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"GE-IN", "dict":"DETECTOR", "idict":22,"hlp":"Germanium intrinsic detector"}]
	,"x4freetext":[" The gamma activity of the irradiated samples"
		," was measured using a Ge detector. The detector"
		," was shielded against direct neutrons from the source"
		," using iron, concrete, polythene and lead."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[" A NE213 liquid scintillation detector was"
		," positioned at 75 cm from the Ge detector in order"
		," to monitor the neutron energy spectrum."]
	}
      ]
     ,"PART-DET":[
	{"x4pointer":" "
	 ,"x4codes":["DG"]
	}
	,{"x4pointer":" "
	 ,"x4codes":["N"]
	}
      ]
     ,"ERR-ANALYS":[
	{"x4pointer":" "
	 ,"x4code":{"header":"DATA-ERR"
	     ,"headerHelp":"Error in value of quantity, defined under ERR-ANALYS"}
	,"x4freetext":[" Error on the experimental values."
		," No details given."]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":[".Data was corrected for gamma-ray background"
		," contributions which are expected to be significant."]
	}
      ]
