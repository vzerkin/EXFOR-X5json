	 ,"x4codes":[{"code":"TAGD", "dict":"INC-SOURCE", "idict":19,"hlp":"Electron tagged"}]
	,"x4freetext":["   The  bremsstrahlung monochromator  identifies"
		," photon energies by determining  the  energies  of  an"
		,"electron  before  and  after it emits a bremsstrahlung"
		,"gamma-ray."]
	}
