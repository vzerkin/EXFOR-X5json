     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Targets of Li-6 Fluoride, Ranging in Thickness"
		,"Between 650 and 800 Microgram/Cm**2. Li-6 Fluoride,"
		,"Of 99.32% Li-6 Enrichment, was Evaporated onto"
		,"Aluminium Disks."]
	}
