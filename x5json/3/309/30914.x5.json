	 ,"x4code":{"header":"DATA-ERR"
	     ,"headerHelp":"Error in value of quantity, defined under ERR-ANALYS"}
	,"x4freetext":[" Errors have been considered for:"
		,"  * counting statistics,"
		,"  * energy resolution and"
		,"  * energy calibration of the crystal spectrometer."]
	}
