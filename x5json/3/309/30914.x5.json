     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	,"x4freetext":[" Neutron beams from the heavy-water reactor were"
		,"first collimated and then reached a NaCl(200) mono-"
		,"chromator. Through Bragg diffraction in NaCl(200) the"
		,"monoenergetic 0.0253 eV neutrons were selected. Between"
		,"the NaCl(200) monochromator and the measured sample a"
		,"mechanical monochromator was inserted in order to cut"
		,"off the neutrons with high energy."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Gold foils, purity 99.9%, diameter 4.2 cm, thickness"
		,"1.527g/cm2."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"MANGB", "dict":"METHOD", "idict":21,"hlp":"Manganese bath"}]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"BF3", "dict":"DETECTOR", "idict":22,"hlp":"Boron Trifluoride neutron detector"}]
	,"x4freetext":[" Using 80% enriched boron-10."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"D4PI", "dict":"DETECTOR", "idict":22,"hlp":"4pi detector"}]
	,"x4freetext":[" using manganese sulphate solution as detector"
		,"medium."]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["For the total cross section corrections have been made"
		,"for the secondary-neutron effect and dead time of"
		,"detecting system. For the absorption cross section"
		,"of gold corrections have been made for the reflected-"
		,"neutron effect, secondary-neutron effect, escaped-"
		,"neutron effect and the boundary effect of the sample."]
	}
      ]
     ,"ERR-ANALYS":[
