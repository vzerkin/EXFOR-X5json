     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Measurement of Li-7(n,n`gamma)Li-7-*(478keV) inelastic"
		,"angular distribution derived via the shape analysis"
		,"of the Doppler shifted gamma-ray spectrum (DSM) at"
		,"14.9 MeV."]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"VDG", "dict":"FACILITY", "idict":18,"hlp":"Van de Graaff"}
	    ,{"code":"3CPRBJG", "dict":"INSTITUTE", "idict":3,"hlp":"Peking Univ., Beijing, China, People`s Rep."}]
	,"x4freetext":[" 400 keV neutron generator of the"
		," Institute of Low Energy Nuclear Physics."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"D-T", "dict":"INC-SOURCE", "idict":19,"hlp":"3H(d,n)"}]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Metallic lithium sample, 36.8 mm in diameter and"
		,"28.6 mm in height, enriched to 99.98% in Li-7."
		,"The sample was positioned at 0 degree with respect"
		,"to the deuteron beam axis and at 13.4 cm from the Ti"
		,"target."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Doppler-broadened gamma spectra were measured at 5"
		,"lab angles. From shape and position analysis of these"
		,"the angular distributions of Li-7 recoils and thereby"
		,"the angular distributions of the inelastic scattered"
		,"neutrons were determined."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"TOF", "dict":"METHOD", "idict":21,"hlp":"Time-of-flight"}]
	,"x4freetext":[" Time-of-flight techniques were applied in order"
		,"to suppress the background from neutrons in the gamma"
		,"spectra."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}
	    ,{"code":"HPGE", "dict":"DETECTOR", "idict":22,"hlp":"Hyperpure Germanium detector"}]
	,"x4freetext":[" A plastic scintillator Ne211 was used as"
		,"a monitor of the TOF system."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Monte Carlo simulations of the gamma-ray spectra"
		,"were done which included several corrections."
		,"These simulations, expanded in Legendre polynomials,"
		,"were fitted to the experimental data with the Least"
		,"Squares Fitting method."]
	}
      ]
     ,"CORRECTION":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Several corrections were taken into account, i.e."
		,"  -  The primary neutron energy spread."
		,"  -  The solid angle correction for the sample to"
		,"     neutron target."
		,"  -  The speed reducing effect of recoil Li* before"
		,"     emitting the gamma-ray."
		,"  -  Gamma detector resolution."
		," These corrections were calculated in the Monte-"
		," Carlo simulations mentioned above."]
	}
      ]
     ,"ERR-ANALYS":[
	{"x4pointer":" "
	 ,"x4code":{"header":"DATA-ERR"
	     ,"headerHelp":"Error in value of quantity, defined under ERR-ANALYS"}
	,"x4freetext":[" Uncertainty in the fitting of the data."]
	}
