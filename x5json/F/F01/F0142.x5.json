     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["For the 2- to 8-MeV deuteron energy range pieces of"
		,"1.4-mil rolled beryllium foil were used as a target."
		,"This foil was sandwiched between five 4-mil"
		,"thicknesses of aluminum backing and another of aluminum"
		,"foils which was used to slow the beam to the desired"
		,"energy. This sandwich was then covered by a `hut` of"
		,"three 4-mil thicknesses of aluminum foil."]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"CYCLO", "dict":"FACILITY", "idict":18,"hlp":"Cyclotron"}
	    ,{"code":"1USACHI", "dict":"INSTITUTE", "idict":3,"hlp":"University of Chicago, IL, United States of America"}]
	,"x4freetext":[" The 37-inch 8-Mev cyclotron and"]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"CCW", "dict":"FACILITY", "idict":18,"hlp":"Cockcroft-Walton accelerator"}
	    ,{"code":"1USACHI", "dict":"INSTITUTE", "idict":3,"hlp":"University of Chicago, IL, United States of America"}]
	,"x4freetext":[" the 400-kev Cockroft-Walton accelerator"
		,"supplied the deuterons."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"GEMUC", "dict":"DETECTOR", "idict":22,"hlp":"Geiger-Mueller counter"}]
	,"x4freetext":[" Self-quenching G-M counters containing argon,"
		,"alcohol, and up to two centimeters of hydrogen have"
		,"been used."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["After a bombardment was completed, the beryllium and"
		,"all the aluminum foils were placed in a alumina cup in"
		,"porcelain furnace tube in a hydrogen atmosphere. This"
		,"tube was heated for thirty minutes to 950 to 1000"
		,"degr. After cooling to about 350 degr., the hydrogen,"
		,"now containing the tritium, was pumped into a counter"
		,"already containing 1.8 cm ethylene and 6 cm argon and"
		,"was counted."]
	}
      ]
     ,"ERR-ANALYS":[
	{"x4pointer":" "
	 ,"x4code":{"header":"DATA-ERR"
	     ,"headerHelp":"Error in value of quantity, defined under ERR-ANALYS"}
	,"x4freetext":["  Counting errors and local variations in"
		,"thickness of the thin berryllium foils contributed to"
		,"the errors in the individual mean cross-section"
		,"determination."]
	}
