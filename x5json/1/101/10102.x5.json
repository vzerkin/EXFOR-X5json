     ,"INC-SPECT":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Roughly 60% source neutrons fall within well-defined"
		," high energy peak about 2 MeV wide. Rest spread over"
		," broad low energy tail. Peak selected by time-of-"
		," flight. Mean energy of beam determined to better than"
		," 100 keV."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Pure polyethylene and spectrogaphic grade graphite. H/C"
		," ratio of polyethylene sample chemically determined to"
		," about 0.2%. Samples 63 to 127 mm long."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Transmission geometry, with counter telescopes"
		," before and after sample."]
	}
