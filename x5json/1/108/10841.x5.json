     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"SCIN", "dict":"DETECTOR", "idict":22,"hlp":"Scintillation detector"}]
	,"x4freetext":[" 1-mm thick Li-6 glass scintillator with Li-6"
		,"content uncertainty less than 1% was used for alpha"
		,"particles, neutron flux was measured with 12.7-mm"
		,"Li-6 and 6.6-cm thick by 10-cm diameter NE-110"
		,"detector."]
	}
      ]
     ,"PART-DET":[
