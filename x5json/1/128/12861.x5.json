     ,"TITLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["EXPERIMENTAL DETERMINATION OF ANAMALOUS SCATTERING"
		,"LENGTHS OF SAMARIUM FOR THERMAL NEUTRONS."]
	}
      ]
     ,"FACILITY":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REAC", "dict":"FACILITY", "idict":18,"hlp":"Reactor"}
	    ,{"code":"1USABNL", "dict":"INSTITUTE", "idict":3,"hlp":"Brookhaven National Laboratory, Upton, NY, United States of America"}]
	,"x4freetext":[" BROOKHAVEN HIGH FLUX BEAM REACTOR."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"DIFFR", "dict":"METHOD", "idict":21,"hlp":"Diffraction"}]
	,"x4freetext":[" DIFFRACTION."
		,"  1800 INDEPENDENT REFLECTIONS WERE MEASURED AT A"
		,"  WAVE LENGTH OF 1.3 ANGSTROM AND THE CRYSTAL STRUCTURE"
		,"  REFINED TO A FINAL R-VALUE OF 0.058."
		,"140 REFLECTIONS WITH STRONG SM-CONTRIBUTION WERE"
		,"  MEASURED AT A NUMBER OF WAVELENGTHS BETWEEN 0.827"
		,"  AND 1.300 ANGSTROMS."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"REAC", "dict":"INC-SOURCE", "idict":19,"hlp":"Reactor"}]
	,"x4freetext":[" 002 BE FACE CRYSTAL WAS USED TO OBTAIN THE"
		,"       MONOCHROMATIC NEUTRON BEAM WITH WAVE LENGTHS"
		,"       BETWEEN 0.827 AND 1.300 ANGSTROMS."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["A SINGLE CRYSTAL OF A SODIUM SAMARIUM EDTA COMPLEX"
		,"  NA+(SM(C10H12N2O8).3H2O-5H2O WAS USED."
		,"  SAMPLE AT 37 DEGREES KELVIN IN HELIUM ATMOSPHERE."]
	}
      ]
     ,"ANALYSIS":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["A LEAST-SQUARED ANALYSIS OF THE REAL AND IMAGINARY"
		,"  PARTS OF THE SCATTERING LENGTH IN RELEVANT"
		,"  TERMS WAS PERFORMED TO OBTAIN THE SCATTERING"
		,"  LENGTH FOR NATURAL SAMARIUM."]
	}
      ]
     ,"ASSUMED":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["THE SCATTERING RADIUS WAS ASSUMED TO BE 7.98 FERMI."]
	}
