	 ,"x4codes":[{"code":"MASSP", "dict":"METHOD", "idict":21,"hlp":"Mass spectrometry of a product"}]
	,"x4freetext":[" Further work involved one month cooling,"
		,"chemical processing, mass spectrographic analysis."]
	}
      ]
