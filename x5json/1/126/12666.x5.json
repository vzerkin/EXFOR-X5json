     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["EPITHERMAL NEUTRON CROSS SECTION FOR DEUTERIUM WAS"
		,"  MEASURED FOR RESONANCE NEUTRONS OF INDIUM AND"
		,"  SILVER."
		,"THERMAL NEUTRONS WERE ABSORBED WITH THE HELP OF"
		,"  A CADMIUM FOIL."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["BEO FILTER WAS USED TO REMOVE NEUTRONS WHOSE WAVE"
		,"  LENGTH IS LESS THAN 4.5A. THE AVERAGE WAVE LENGTH"
		,"  OF THE NEUTRONS WAS FOUND TO BE 5.43A."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["LONG BF3 COUNTER"]
	}
