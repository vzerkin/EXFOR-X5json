	,"x4freetext":[" Detector of Recoils and Gammas of"
		,"Nuclear Reactions was used to perform a direct"
		,"measurement of the 26mAl(p,g) reaction at the ISAC-I"
		,"radioactive beam facility at TRIUMF."]
	}
      ]
     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Windowless gas target filled with H2 gas at 6.66 mbar."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"BGO", "dict":"DETECTOR", "idict":22,"hlp":"Bismuth-Germanate crystal detector"}]
	,"x4freetext":[" Array of 30 bismuth germanate (BGO) detectors"
		,"surrounding the gas target was used to detect gamma"
		,"rays resulting from (p,gamma) reactions"]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"MAGSP", "dict":"DETECTOR", "idict":22,"hlp":"Magnetic spectrometer"}]
	,"x4freetext":[" 27Si recoils were momentum analyzed by DRAGON"
		,"magnetic and electrostatic dipole systems and"
		,"transmitted to the focal plane of DRAGON."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"TELES", "dict":"DETECTOR", "idict":22,"hlp":"Counter telescope"}
	    ,{"code":"MCPLT", "dict":"DETECTOR", "idict":22,"hlp":"Microchannel plate"}
	    ,{"code":"IOCH", "dict":"DETECTOR", "idict":22,"hlp":"Ionization chamber"}]
	,"x4freetext":["Recoils were identified using a local"
		,"time-of-flight (TOF) system based on two microchannel"
		,"plates and an isobutane-filled ionization chamber."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"NAICR", "dict":"DETECTOR", "idict":22,"hlp":"Sodium-Iodide crystal"}]
	,"x4freetext":[" Positrons from decay of 26mAl were guided to"
		,"the pair of NaI(Tl) detectors. By comparing the number"
		,"of coincident 511-keV annihilation rays with GEANT4"
		,"simulations of the NaI detector response, an average"
		,"26mAl beam intensity of was determined"]
	}
      ]
