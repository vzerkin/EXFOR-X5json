     ,"SAMPLE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["The 327 mu-g/cm2 -thick 28Si secondary emission foil"
		,"of the microchannel plate (MCP) detector served as"
		,"the target for the experiment."]
	}
      ]
     ,"DETECTOR":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"MCPLT", "dict":"DETECTOR", "idict":22,"hlp":"Microchannel plate"}]
	,"x4freetext":[" Two microchannel plate detectors, one at the"
		,"target position, the second one 1.3 m upstream"
		,"provided a time-of-flight measurement of the beam"
		,"particles."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"IOCH", "dict":"DETECTOR", "idict":22,"hlp":"Ionization chamber"}]
	,"x4freetext":[" Two compact axial field ionization chambers"
		,"were inserted in the beam path between the two MCP"
		,"detectors for particle identification."]
	}
	,{"x4pointer":" "
	 ,"x4codes":[{"code":"SI", "dict":"DETECTOR", "idict":22,"hlp":"Silicon detector"}]
	,"x4freetext":[" Two annular silicon detectors which  subtend the"
		,"laboratory angles 1.0-7.3deg were used for detection"
		,"of evaporation residui."]
	}
      ]
     ,"METHOD":[
	{"x4pointer":" "
	 ,"x4codes":[{"code":"TOF", "dict":"METHOD", "idict":21,"hlp":"Time-of-flight"}]
	,"x4freetext":[" Time of flight allowed rejection of beam"
		,"particles scattered or degraded prior to the target"
		,"and provided a direct measure of the number of beam"
		,"particles incident on the target."
		,"Particle identification was achieved by E-TOF"
		,"method."]
	}
      ]
     ,"INC-SOURCE":[
	{"x4pointer":" "
	 ,"x4code":null
	,"x4freetext":["Ions of 39 K from a source or 47 K from a thermalized"
		,"radioactive beam were charge bred in an ion trap,"
		,"injected into the ReA3 80-MHz linac, and"
		,"re-accelerated to energies between 2 and 3 MeV/A."]
	}
