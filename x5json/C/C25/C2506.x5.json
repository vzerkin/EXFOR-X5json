	}
	,{"x4pointer":" "
	 ,"x4code":{"header":"ERR-5"
	     ,"headerHelp":"5th partial uncertainty, defined under ERR-ANALYS"}
	,"x4freetext":[" uncertainty in target thickness (5%),"]
	}
	,{"x4pointer":" "
	 ,"x4code":{"header":"ERR-6"
	     ,"headerHelp":"6th partial uncertainty, defined under ERR-ANALYS"}
	,"x4freetext":[" uncertainty in the correction for annihilation"
